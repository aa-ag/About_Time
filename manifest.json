{
    "manifest_version": 2,
    "name": "About Time",
    "description": "About Time tells you how many minutes it'll take you to read a website.",
    "version": "0.1.0",
    "icons": { "128": "icon_128.png" },
    "offline_enabled": true,
    "permissions": [ 
      "tabs", 
      "activeTab",
      "background",
      "http://*/*",
      "https://*/*"
    ],
    "content_scripts": [
        {
        "matches": [ "<all_urls>" ],
        "run_at": "document_end",
        "js": [ "content.js" ],
        "css": [ "style.css" ]
    }
    ],
    "background":{ "scripts": ["background.js"] },
    "browser_action": {
        "default_icon": {
          "16": "icon_128.png"
        }
      },
    "author": "Aaron A."

}